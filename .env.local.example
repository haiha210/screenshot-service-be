# Local Development with LocalStack
# Copy this to .env.local and use: NODE_ENV=development USE_LOCALSTACK=true node scripts/send-test-message.js

USE_LOCALSTACK=true
AWS_ENDPOINT=http://localhost:4566
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test

# SQS Configuration
SQS_QUEUE_URL=http://localhost:4566/000000000000/screenshot-queue

# S3 Configuration
S3_BUCKET_NAME=screenshot-bucket

# DynamoDB Configuration
DYNAMODB_TABLE_NAME=screenshot-results
