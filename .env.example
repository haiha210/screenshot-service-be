# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=local_access_key_id
AWS_SECRET_ACCESS_KEY=local_secret_access_key
AWS_ENDPOINT=http://localstack:4565

# SQS Configuration
SQS_QUEUE_URL=http://localstack:4566/000000000000/screenshot-queue
SQS_BATCH_SIZE=5
SQS_VISIBILITY_TIMEOUT=300
SQS_WAIT_TIME_SECONDS=20

# S3 Configuration
S3_BUCKET_NAME=screenshot-bucket
S3_SCREENSHOT_PREFIX=screenshots/

# DynamoDB Configuration
DYNAMODB_TABLE_NAME=screenshot-results

# Screenshot Configuration
SCREENSHOT_WIDTH=1920
SCREENSHOT_HEIGHT=1080
SCREENSHOT_TIMEOUT=30000
SCREENSHOT_FORMAT=png

# Application Configuration
NODE_ENV=development
LOG_LEVEL=info
USE_LOCALSTACK=true
